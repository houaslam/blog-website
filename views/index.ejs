<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head.ejs') %>
<body>
    <%- include('./partials/nav.ejs') %>
    <main>
        <% if (blogs.length > 0) {%>
            <%blogs.forEach(blog => { %>
                <div class="blog">
                    <h2><%= blog.title %></h2>
                    <h3><%= blog.snippet %></h3>
                    <p><%= blog.body%></p>
                </div>
            <% }) %>
        <% } else { %>
            <p>no Blog was created...</p>
        <% } %>
    </main>
    <%- include('./partials/footer.ejs') %>

</body>
</html>